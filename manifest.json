{
    "manifest_version": 3,
    "name": "AI DETECTOR",
    "description": "A Chrome extension that predicts whether text is Ai generated or Human written",
    "version": "1.0",
    
    "action": {
      "default_icon" :"icon.png",
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://127.0.0.1:5000/"
    ],
    "icons": {
      "128": "icon.png"
  },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "web_accessible_resources": [
      {
        "resources": ["popup.js"],
        "matches": ["http://*/*", "https://*/*"]
      }
    ]
  }